{
  "name": "npm-fuzzy",
  "version": "1.0.1",
  "type": "module",
  "description": "Fast fuzzy string matching library for JavaScript/TypeScript with 10/10 performance score",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit --strict",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "check": "npm run typecheck && npm run build && npm run lint && npm run format:check && npx knip",
    "prepublishOnly": "npm run check && npm run build"
  },
  "keywords": [
    "fuzzy",
    "string",
    "matching",
    "levenshtein",
    "similarity",
    "search",
    "rapidfuzz",
    "fuzzy-search",
    "string-similarity",
    "autocomplete",
    "text-matching"
  ],
  "author": {
    "name": "monarch labs inc",
    "email": "monarchlabstech@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bantoinese83/npm-fuzzy.git"
  },
  "bugs": {
    "url": "https://github.com/bantoinese83/npm-fuzzy/issues"
  },
  "homepage": "https://github.com/bantoinese83/npm-fuzzy#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "knip": "^5.0.0",
    "prettier": "^3.1.1",
    "rollup": "^4.6.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=14"
  }
}
